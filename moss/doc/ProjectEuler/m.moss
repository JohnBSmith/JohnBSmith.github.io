
use nt: isprime

sub prime(n)
  if n<=3
    return n>1
  elif n%2==0 or n%3==0
    return false
  else
    i=5
    while i*i<=n
      if n%i==0 or n%(i+2)==0
        return false
      end
      i+=6
    end
    return true
  end
end

sub main
  k=0
  for p in (1..1000000).filter(prime)
    if k%1000==0
      print(p)
    end
    k++
  end
end

